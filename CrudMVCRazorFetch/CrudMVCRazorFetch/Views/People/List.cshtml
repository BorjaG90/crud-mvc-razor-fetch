@using CrudMVCRazorFetch.Models.ViewModel
@model List<ListPeopleViewModel>
@{
    Layout = null;
}

@foreach (var oPeople in Model)
{
<div>
    Nombre: @oPeople.Name<br />
    Edad: @oPeople.Age
    <input type="button" value="Editar" onclick="document.location.href='@Url.Content("~/People/Edit/?Id="+oPeople.Id)'"/>
    <input type="button" value="Eliminar" onclick="jsDelete(@oPeople.Id)" />
</div>
}
